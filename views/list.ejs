<%- include("header") -%>  
    <div class="box" id="heading">
        <h1>
            <%= kindOfDay %>
        </h1>
    </div>
    <div class="box">
        <% for (var i=0;i < itemsLength;i++) { %>
            <div class="item">
                <input type="checkbox" >
                <p>
                    <%= listItems[i] %>
                <p>
            </div>
            <% } %>

            <form class="item" action="/" method="post">
                <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
                <button type="submit" name="button1" id="subButton" value= <%= kindOfDay %> >+</button>
            </form>
    </div>


<%- include("footer") -%>
